<%= form_for @user do |f| %>
      Enter First Name <%= f.text_field :fname %>
      <br />
      Enter Last Name <%= f.text_field :lname %>
       <br />
      Enter Email Address <%= f.text_field :email %>
       <br />
      Enter Password <%= f.password_field :password %>
       <br />
      Enter a Security Question <%= f.text_area :sec_ques %>
       <br />
      Enter an Answer to Your Question <%= f.text_area :sec_ans %>
       <br />
      <%= f.submit "Create" %>
    <% end %>